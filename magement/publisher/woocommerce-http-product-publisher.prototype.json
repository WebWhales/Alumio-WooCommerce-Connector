{
  "$schema": "https://di.schema.mediact.com/register.prototype.json",
  "type": "publisher",
  "identifier": "woocommerce-http-product-publisher",
  "name": "WooCommerce HTTP Product Publisher",
  "object": {
    "class": "Magement\\HttpConnector\\HttpPublisher",
    "arguments": {
      "client": {
        "class": "WebWhales\\AlumioWooCommerceConnector\\DataContainerHttpClient\\EntityWoocommerceClient",
        "arguments": {
          "client": "%{client}",
          "object_name": "products",
          "object_id": "%{product_id}",
          "extra_object_name": "variations",
          "extra_object_id": "%{variation_id}",
              "payload": "&{@}"
        },
        "meta": {
          "payload": {
            "default": "&{@}"
          }
        }
      }
    }
  },
  "schema": {
    "type": "object",
    "properties": {
      "product_id": {
        "type": "string",
        "title": "Product id",
        "ui": {
          "ui:help": "Supports &{} placeholders"
        }
      },
      "variation_id": {
        "type": "string",
        "title": "Variation id",
        "ui": {
          "ui:help": "Supports &{} placeholders"
        }
      },

      "client": {
        "type": "string",
        "title": "HTTP Client",
        "default": "default",
        "magement": {
          "type": "http-client"
        }
      }

    },
    "required": [
      "product_id",
      "client"
    ]
  }
}