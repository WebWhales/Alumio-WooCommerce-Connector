{
    "name": "webwhales/alumio-woocommerce-connector",
    "description": "WooCommerce connector for Alumio",
    "homepage": "https://github.com/WebWhales/Alumio-WooCommerce-Connector",
    "type": "library",
    "license": "proprietary",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "magement/file-queue-subscriber": "^1.1",
        "magement/http-client": "^4.0@RC",
        "magement/http-connector": "^2.1",
        "magement/publisher-api": "^1.0",
        "magement/storage-connector": "^1.2",
        "magement/subscriber-api": "^1.0",
        "mediact/http-server-handler-manager": "^1.0",
        "nesbot/carbon": "^2.50"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.mediact.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "WebWhales\\AlumioWooCommerceConnector\\": "./src"
        }
    },
    "support": {
        "issues": "https://github.com/WebWhales/Alumio-WooCommerce-Connector/issues"
    },
    "require-dev": {
        "mediact/testing-suite": "^2.9"
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/grumphp.yml",
            "/pdepend.xml",
            "/phpstan.neon",
            "/phpunit.xml",
            "/phpcs.xml",
            "/phpmd.xml",
            "/tests"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
